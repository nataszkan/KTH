{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "node-api-ref"
    },
    "secretNames": {
      "value": ["API_KEYS_0", "MONGODB_URI"]
    },
    "environmentVariables": {
      "value": {
        "API_VERSION": "^2.0.0",
        "CONSOLE_ENABLED": "false",
        "LOGGING_ACCESS_LOG": "false",
        "LOGGING_LEVEL": "info",
        "NODE_ENV": "production",
        "SERVER_PORT": "3001",
        "SERVICE_PUBLISH": "/api/node",
        "STDOUT_ENABLED": "true"
      }
    },
    "monitorPath": {
      "value": "/api/node/_monitor"
    },
    "websitePort": {
      "value": "3001"
    }
  }
}
